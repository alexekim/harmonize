<img id="record" alt="">

{{#each musicians}}
<div class="container">
  <div style="background-color: rgba(85, 214, 148, 0.7); margin: 5%;" class="row" id="eachmusician">
    <div class="col-xs-2">
      <div style="position: absolute; top: 50%;"><img id="browsepreview" src="{{this.images}}" alt="" /></div>
    </div>
    <div class="col-xs-10">
      <h2>{{this.username}}: {{this.name}}, of {{this.act}}</h2>
      <h4>{{this.location}}</h4>
      <h5>Plays: {{this.primary}}</h5>
      <h6>Also plays: {{this.secondary}}</h6>
      <h5>Aspirations: {{this.aspirations}}</h5>
      <p>Keywords: {{this.keywords}}</p>
      <p>
        <a href="/users/{{username}}">See More</a>
      </p>

    </div>
  </div>
</div>

<!-- <h2>{{{this.video}}}</h2> -->
<!-- <h2>{{this.genres}}</h2>
<h2>{{this.links}}</h2> -->
{{/each}}
